<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd" version="3.1">
  <display-name>autofinanzierung </display-name>
  <enterprise-beans>
    <session>
      <ejb-name>CarFinancingHandlerBean</ejb-name>
      <env-entry>
        <env-entry-name>interestRate</env-entry-name>
        <env-entry-type>java.lang.Double</env-entry-type>
        <env-entry-value>0.01</env-entry-value>
      </env-entry>
    </session>
  </enterprise-beans>
  <ejb-client-jar>autofinanzierung-common.jar</ejb-client-jar>
</ejb-jar>